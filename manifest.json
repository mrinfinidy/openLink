{
    "manifest_version": 2,
    "name": "Open Links",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["csLinks.js"]
        }
    ],
    "background": {
        "scripts": ["interaction.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}
